<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.zhuchl.mybatis.dao.mapper.AddressMapper">

    <select id="findOne" resultType="Address">
        SELECT * FROM address_info WHERE user_acco=#{userAcco} limit 1
    </select>

    <select id="findColl" resultType="Address">
        SELECT * FROM address_info WHERE user_acco=#{userAcco}
    </select>


</mapper>